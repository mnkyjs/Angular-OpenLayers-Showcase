{
    "name": "open-layers-showcase",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:ci": "ng build --base-href=/Angular-OpenLayers-Showcase/ && npm run copy:docs",
        "copy:docs": "copyfiles -u 1 dist/**/* docs",
        "watch": "ng build --watch --configuration development",
        "test": "ng test --watch=false --browsers=ChromeHeadless",
        "lint": "ng lint",
        "analyze": "ng build --stats-json --output-hashing none --named-chunks && esbuild-visualizer --template treemap --metadata dist/stats.json --filename dist/analyse/index.html && http-server -o -c-1 ./dist/analyse/",
        "analyze:sme": "ng build --source-map --output-hashing none --named-chunks && source-map-explorer dist/browser/*.js --html dist/sme/index.html && http-server -o -c-1 ./dist/sme/"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.2.0",
        "@angular/cdk": "^17.2.1",
        "@angular/common": "^17.2.0",
        "@angular/compiler": "^17.2.0",
        "@angular/core": "^17.2.0",
        "@angular/forms": "^17.2.0",
        "@angular/material": "^17.2.1",
        "@angular/platform-browser": "^17.2.0",
        "@angular/platform-browser-dynamic": "^17.2.0",
        "@angular/platform-server": "^17.2.0",
        "@angular/router": "^17.2.0",
        "@angular/ssr": "^17.2.1",
        "express": "^4.18.2",
        "ol": "7.5.2",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.2.1",
        "@angular-eslint/builder": "17.2.1",
        "@angular-eslint/eslint-plugin": "17.2.1",
        "@angular-eslint/eslint-plugin-template": "17.2.1",
        "@angular-eslint/schematics": "17.2.1",
        "@angular-eslint/template-parser": "17.2.1",
        "@angular/cli": "^17.2.1",
        "@angular/compiler-cli": "^17.2.0",
        "@types/express": "^4.17.17",
        "@types/jasmine": "~5.1.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^18.18.0",
        "@typescript-eslint/eslint-plugin": "6.19.0",
        "@typescript-eslint/parser": "6.19.0",
        "autoprefixer": "^10.4.18",
        "copyfiles": "^2.4.1",
        "esbuild-visualizer": "^0.6.0",
        "eslint": "^8.56.0",
        "http-server": "^14.1.1",
        "jasmine-core": "~5.1.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "postcss": "^8.4.35",
        "prettier": "^3.2.5",
        "source-map-explorer": "^2.5.3",
        "tailwindcss": "^3.4.1",
        "typescript": "~5.3.2",
        "webpack-bundle-analyzer": "4.10.1"
    }
}
