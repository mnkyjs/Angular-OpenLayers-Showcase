{
    "name": "open-layers-showcase",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:ci": "ng build --output-path=docs --base-href=/Angular-OpenLayers-Showcase/ --configuration production",
        "watch": "ng build --watch --configuration development",
        "test": "ng test --watch=false --browsers=ChromeHeadless",
        "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
        "serve:ssr:open-layers-showcase": "node dist/open-layers-showcase/server/server.mjs",
        "build:analyse": "ng build --source-map --named-chunks && source-map-explorer dist/open-layers-showcase/browser*/main.*.js",
        "lint": "ng lint"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.2.0",
        "@angular/cdk": "^17.2.1",
        "@angular/common": "^17.2.0",
        "@angular/compiler": "^17.2.0",
        "@angular/core": "^17.2.0",
        "@angular/forms": "^17.2.0",
        "@angular/material": "^17.2.1",
        "@angular/platform-browser": "^17.2.0",
        "@angular/platform-browser-dynamic": "^17.2.0",
        "@angular/platform-server": "^17.2.0",
        "@angular/router": "^17.2.0",
        "@angular/ssr": "^17.2.1",
        "express": "^4.18.2",
        "ol": "7.5.2",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.2.1",
        "@angular-eslint/builder": "17.2.1",
        "@angular-eslint/eslint-plugin": "17.2.1",
        "@angular-eslint/eslint-plugin-template": "17.2.1",
        "@angular-eslint/schematics": "17.2.1",
        "@angular-eslint/template-parser": "17.2.1",
        "@angular/cli": "^17.2.1",
        "@angular/compiler-cli": "^17.2.0",
        "@types/express": "^4.17.17",
        "@types/jasmine": "~5.1.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^18.18.0",
        "@typescript-eslint/eslint-plugin": "6.19.0",
        "@typescript-eslint/parser": "6.19.0",
        "eslint": "^8.56.0",
        "jasmine-core": "~5.1.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "prettier": "^3.2.5",
        "source-map-explorer": "2.5.3",
        "typescript": "~5.3.2",
        "webpack-bundle-analyzer": "4.10.1"
    }
